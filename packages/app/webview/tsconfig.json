{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "verbatimModuleSyntax": true,
    "types": ["vite/client"],
    "allowSyntheticDefaultImports": true,
    "noEmit": true
  },
  "include": [
    "src",
    "vite.config.ts",
    "tailwind.config.ts"
  ]
}
